#include <stdio.h>
#include <math.h>

int main(void) {
   double x1;
   double y1;
   double x2;
   double y2;
   double xDist;
   double yDist;
   double pointsDistance;

   xDist = 0.0;
   yDist = 0.0;
   pointsDistance = 0.0;

   scanf("%lf", &x1);
   scanf("%lf", &y1);
   scanf("%lf", &x2);
   scanf("%lf", &y2);

   pointsDistance = sqrt ( pow ((x2 - x1) , 2.0) + pow ( (y2 - y1) , 2.0));

   printf("%lf\n", pointsDistance);

   return 0;
}
