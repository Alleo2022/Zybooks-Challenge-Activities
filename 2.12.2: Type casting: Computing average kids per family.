#include <stdio.h>

int main(void) {
   int numKidsA;
   int numKidsB;
   int numKidsC;
   int numFamilies;
   double avgKids;

   scanf("%d", &numKidsA);
   scanf("%d", &numKidsB);
   scanf("%d", &numKidsC);
   scanf("%d", &numFamilies);

   avgKids = (double) (numKidsA + numKidsB + numKidsC) / (double) (numFamilies);
   
   printf("%lf\n", avgKids);

   return 0;
}
