#include <stdio.h>
#include <stdbool.h>

int main(void) {
   int userNum;
   bool isPositive;
   bool isEven;

   scanf("%d", &userNum);
   isPositive = (userNum > 0);
   isEven = ((userNum % 2) == 0);

   if ((isEven == true) && (isPositive == true)) {
      printf ("Positive even number\n");
   }
   else if ((isPositive == true) && (!isEven == true)){
      printf ("Positive number\n");
   }
   else {
      printf ("Not a positive number\n");
   }

   return 0;
}
